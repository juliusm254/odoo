<odoo>

<template id="rental_units_list_template" name="Rental Units List">
    <div id="wrap" class="container">
        <h1>Rental Units</h1>
        <t t-foreach="units" t-as="unit">
            <div class="row">
                <span t-field="unit.name" />,
                <span t-field="unit.partner_id" />,
                <span t-field="unit.price" />
            </div>
        </t>
    </div>
</template>

</odoo>
