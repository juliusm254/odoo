<odoo>
    <data>
        <record id="property_property_form_view" model="ir.ui.view">
            <field name="name">property.property.form.view</field>
            <field name="model">property.property</field>
            <field name="inherit_id" ref="property_mgmt.property_property_form_view" />
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page name="units" string="Related Units">
                        <field name="unit_ids" readonly="0">
                            <tree editable="bottom">
                                <field name="name" />
                                <field name="partner_id" />
                                <field name="price" />
                                <field name="is_occupied" />
                                <!-- <field name="partner_status" />
                                <field name="stakeholder_tax_unit" />
                                <field name="owner_numerator" />
                                <field name="owner_denominator" />
                                <field name="percentage" /> -->
                            </tree>
                        </field>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</odoo>
