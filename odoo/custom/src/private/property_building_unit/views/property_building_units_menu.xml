<odoo>
    <!-- Library App Menu -->
    <menuitem id="menu_rentals" name="Rental Units" />


        <!-- Action to open the RentalsUnits list -->
    <record id="action_rental_units" model="ir.actions.act_window">
        <field name="name">Rental Units</field>
        <field name="res_model">rental.unit</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu Item to open the Units list -->
    <menuitem
        id="menu_rental_units"
        name="Units"
        parent="menu_rentals"
        action="action_rental_units"
    />


</odoo>
